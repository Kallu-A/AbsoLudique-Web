    "prod:build": "NODE_ENV=production yarn run build:css && yarn run build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.11",
    "autoprefixer": "^10.4.13",
    "next": "latest",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.0.3",
    "tailwindcss": "^3.2.6"
  }
}
